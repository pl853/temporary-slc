               rows=\\\"20\\\"\\n                type=\\\"textarea\\\"\\n                onFocus={e => this.setState({ addressFocus: true })}\\n                onBlur={e => this.setState({ addressFocus: false })}\\n              />\\n            </InputGroup>\\n            \\n          </Col>\\n        </Row>\\n      </>\\n    );\\n  }\\n}\\n\\nexport default Wizard;\\n\",\"import React from \\\"react\\\";\\n\\n// reactstrap components\\nimport {\\n  Col,\\n  Row,\\n  Card,\\n  CardTitle,\\n  CardHeader,\\n} from \\\"reactstrap\\\";\\n\\n\\n\\nclass UnderConstruction extends React.Component {\\n  constructor(props) {\\n    super(props);\\n    this.state = {\\n    };\\n  }\\n  render() {\\n    return (\\n      <>\\n        <div className=\\\"content underConstruction\\\">\\n          <Row className=\\\"mt-5\\\">\\n            <Col className=\\\"ml-auto mr-auto mt-5\\\" md=\\\"6\\\">\\n              <Card className=\\\"card card-subcategories mt-5 mb-0\\\">\\n                <CardHeader className=\\\"\\\">\\n                  <CardTitle className=\\\"text-center\\\" tag=\\\"h1\\\">\\n                    <h1 className=\\\"title text-center mt-1\\\">UNDER CONSTRUCTION &nbsp; <i className=\\\"tim-icons icon-settings-gear-63 mb-1\\\"></i> </h1>\\n                    <h3 className=\\\"text-center mt-1\\\">This page will be available soon!</h3>\\n                  </CardTitle>\\n                </CardHeader>\\n              </Card>\\n            </Col>\\n          </Row>\\n        </div>\\n      </>\\n    );\\n  }\\n}\\n\\nexport default UnderConstruction;\\n\",\"import React, { Component } from \\\"react\\\";\\n//import classNames from \\\"classnames\\\";\\n// react plugin that creates an input with badges\\nimport TagsInput from \\\"react-tagsinput\\\";\\n// reactstrap components\\nimport {\\n  Card,\\n  CardHeader,\\n  CardBody,\\n  CardTitle,\\n  CardFooter,\\n  TabContent,\\n  TabPane,\\n  Col,\\n  Row,\\n  Button\\n} from \\\"reactstrap\\\";\\n\\nimport { withFirebase } from \\\"../../components/Firebase\\\";\\nimport UnderConstruction from \\\"../UnderConstruction\\\";\\n\\nclass ProjectPage extends Component {\\n  constructor(props) {\\n    super(props);\\n    this.state = {\\n      horizontalTabs: \\\"allprojects\\\"\\n    };\\n  }\\n  changeActiveTab = (e, tabState, tadName) => {\\n    e.preventDefault();\\n    this.setState({\\n      [tabState]: tadName\\n    });\\n  };\\n\\n  render() {\\n    return (\\n      <>\\n        <div className=\\\"content projects\\\">\\n\\n            <Card className=\\\"card-plain\\\">\\n              {/* <CardHeader>\\n                <ButtonGroup\\n                  className=\\\"btn-group-toggle float-left\\\"\\n                  data-toggle=\\\"buttons\\\"\\n                >\\n                  <Button\\n                    color=\\\"primary\\\"\\n                    id=\\\"0\\\"\\n                    size=\\\"sm\\\"\\n                    tag=\\\"label\\\"\\n                    className={classNames(\\\"btn-simple\\\", {\\n                      active:\\n                        this.state.horizontalTabs === \\\"allprojects\\\"\\n                          ? \\\"active\\\"\\n                          : \\\"\\\"\\n                    })}\\n                    onClick={e =>\\n                      this.changeActiveTab(e, \\\"horizontalTabs\\\", \\\"allprojects\\\")\\n                    }\\n                  >\\n                    <input defaultChecked name=\\\"options\\\" type=\\\"radio\\\" />\\n                    <span className=\\\"d-none d-sm-block d-md-block d-lg-block d-xl-block\\\">\\n                      All Projects\\n                    </span>\\n                    <span className=\\\"d-block d-sm-none\\\">\\n                      <i className=\\\"tim-icons icon-single-02\\\" />\\n                    </span>\\n                  </Button>\\n                  <Button\\n                    color=\\\"primary\\\"\\n                    id=\\\"1\\\"\\n                    size=\\\"sm\\\"