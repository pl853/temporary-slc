"content\\\">\\n        {loading && <div>Loading ...</div>}\\n        {projects && (\\n          <ProjectList\\n            projects={projects.map(project => ({\\n              ...project\\n            }))}\\n          />\\n        )}\\n        {!projects && (\\n          <div>\\n            <UnderConstruction />\\n          </div>\\n        )}\\n      </div>\\n    );\\n  }\\n}\\n\\nconst ProjectList = ({ projects }) => (\\n  <Row>\\n    {projects.map(project => (\\n      <ProjectItem key={project.uid} project={project} />\\n    ))}\\n  </Row>\\n);\\n\\nclass ProjectItem extends Component {\\n  constructor(props) {\\n    super(props);\\n\\n    this.state = {\\n      tagsinput: [\\n        this.props.project.tag1,\\n        this.props.project.tag2,\\n        this.props.project.tag3,\\n        this.props.project.tag4\\n      ] //GOTTA MAP THOSE TAGS\\n    };\\n  }\\n\\n  handleTagsinput = tagsinput => {\\n    this.setState({ tagsinput });\\n  };\\n\\n  render() {\\n    const { project } = this.props;\\n\\n    return (\\n      <Col md=\\\"6\\\" lg=\\\"4\\\">\\n        <Card className=\\\"portfolio-card\\\">\\n          <CardHeader>\\n            <img alt=\\\"\\\" src={project.urlPreview} />\\n            <h4 className=\\\"card-category mt-2\\\">{project.category}</h4>\\n            <CardTitle tag=\\\"h3\\\">{project.title}</CardTitle>\\n          </CardHeader>\\n          <CardBody>\\n            <p>{project.description}</p>\\n\\n            <TagsInput\\n              onChange={this.handleTagsinput}\\n              tagProps={{\\n                className: \\\"react-tagsinput-tag primary\\\"\\n              }}\\n              value={this.state.tagsinput}\\n            />\\n          </CardBody>\\n          <CardFooter>\\n            <Button className=\\\"btn-icon btn-simple\\\" color=\\\"github\\\" href={project.urlGit}>\\n                        <i className=\\\"fab fa-github\\\" />\\n                      </Button>\\n            <Button className=\\\"btn-simple\\\" co