    title\\n                  headerTextCenter\\n                  finishButtonText=\\\"finish\\\"\\n                  finishButtonClick={() => this.submitForm()}\\n                  finishButtonClasses=\\\"btn-wd btn-primary\\\"\\n                  nextButtonClasses=\\\"btn-wd btn-primary\\\"\\n                  previousButtonClasses=\\\"btn-wd btn-primary\\\"\\n                  progressbar\\n                  color=\\\"primary\\\"\\n                />\\n              </Col>\\n            </Fade>\\n          </Form>\\n        </div>\\n      </>\\n    );\\n  }\\n}\\n\\nexport default Wizard;\\n\",\"import React from \\\"react\\\";\\n\\n// reactstrap components\\nimport {\\n  Col,\\n  Row,\\n  Card,\\n  CardTitle,\\n  CardHeader,\\n} from \\\"reactstrap\\\";\\n\\n\\n\\nclass UnderConstruction extends React.Component {\\n  constructor(props) {\\n    super(props);\\n    this.state = {\\n    };\\n  }\\n  render() {\\n    return (\\n      <>\\n        <div className=\\\"content underConstruction\\\">\\n          <Row className=\\\"mt-5\\\">\\n            <Col className=\\\"ml-auto mr-auto mt-5\\\" md=\\\"6\\\">\\n              <Card className=\\\"card card-subcategories mt-5 mb-0\\\">\\n                <CardHeader className=\\\"\\\">\\n                  <CardTitle className=\\\"text-center\\\" >\\n                    <h1 className=\\\"title text-center mt-1\\\">UNDER CONSTRUCTION &nbsp; <i className=\\\"tim-icons icon-settings-gear-63 mb-1\\\"></i> </h1>\\n                    <h3 className=\\\"text-center mt-1\\\">This page will be available soon!</h3>\\n                  </CardTitle>\\n                </CardHeader>\\n              </Card>\\n            </Col>\\n          </Row>\\n        </div>\\n      </>\\n    );\\n  }\\n}\\n\\nexport default UnderConstruction;\\n\",\"import React, { Component } from \\\"react\\\";\\n//import classNames from \\\"classnames\\\";\\n// react plugin that creates an input with badges\\nimport TagsInput from \\\"react-tagsinput\\\";\\nimpo